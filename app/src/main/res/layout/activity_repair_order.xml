        android:inputType="text"
        tools:layout_editor_absoluteX="208dp"
        tools:layout_editor_absoluteY="293dp" />

    <EditText
        android:id="@+id/editTextText3"
        android:layout_width="162dp"
        android:layout_height="40dp"
        android:ems="10"
